package com.company;

public class Cashier {

    private String nameCashier;
    private final int ID;
    private final double monthlySalary;

    public Cashier(String nameCashier, int ID, double monthlySalary) {
        this.nameCashier = nameCashier;
        this.ID = ID;
        this.monthlySalary = monthlySalary;
    }

    public int getID() {
        return ID;
    }

    public String getNameCashier() {
        return nameCashier;
    }

    public void setNameCashier(String nameCashier) {
        this.nameCashier = nameCashier;
    }

    @Override
    public String toString() {
        return "Cashier{" +
                "nameCashier='" + nameCashier + '\'' +
                ", ID=" + ID +
                ", monthlySalary=" + monthlySalary +
                '}';
    }
}
