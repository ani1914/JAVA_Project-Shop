package com.company;

public enum TypeGood {
    Food(1),NonFood(2);
    private int percentageIncrease;

    TypeGood(int percentageIncrease) {
        this.percentageIncrease = percentageIncrease;
    }

    public int getPercentageIncrease() {
        return percentageIncrease;
    }

    public void setPercentageIncrease(int percentageIncrease) {
        this.percentageIncrease = percentageIncrease;
    }
}
